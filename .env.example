# Environment (.env) example for WeatherApi
# Copy this file to .env and fill real values before running

# NODE
NODE_ENV=development
PORT=3000

# Database (Prisma) - PostgreSQL connection string
# Example: postgresql://user:password@localhost:5432/weatherdb
DATABASE_URL=

# JWT secrets & expirations
JWT_SECRET=
JWT_REFRESH_SECRET=
JWT_EXPIRES_IN=15m
JWT_REFRESH_EXPIRES_IN=7d

# External APIs
OPENWEATHER_API_KEY=